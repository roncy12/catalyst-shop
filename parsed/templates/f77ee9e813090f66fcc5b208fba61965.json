{
  "components/themes/sections/amp-section": "<ul class=\"navPage-subMenu-list\">\r\n    <li class=\"navPage-subMenu-item\">\r\n        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\r\n    </li>\r\n    {{#each children}}\r\n    <li class=\"navPage-subMenu-item\">\r\n        {{#if children}}\r\n        <a class=\"navPage-subMenu-action navPages-action has-subMenu\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\" data-collapsible-disabled-breakpoint=\"medium\"\r\n            data-collapsible-disabled-state=\"open\" data-collapsible-enabled-state=\"closed\">\r\n            {{name}}\r\n            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-chevron-down\" />\r\n                </svg>\r\n            </i>\r\n        </a>\r\n        <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n            {{#each children}}\r\n            <li class=\"navPage-childList-item\">\r\n                <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n        {{else}}\r\n        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n        {{/if}}\r\n    </li>\r\n    {{/each}}\r\n</ul>\n"
}
