{
  "components/common/navigation-mega": "{{#if navigation-mega '==' 'megamenu'}}  \r\n{{#if id '==' ../mega_category_id}}\r\n     <ul class=\"navPage-subMenu-list col-md-{{lang 'mega-menu.block-mega-1.col-menu-sub' }} {{lang 'mega-menu.block-mega-1.on-off-menusub'}}\">\r\n     Check mega\r\n        <li class=\"navPage-subMenu-item\">\r\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a\r\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\r\n                        href=\"{{url}}\"\r\n                        data-collapsible=\"navPages-{{id}}\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\"\r\n                        data-collapsible-disabled-state=\"open\"\r\n                        data-collapsible-enabled-state=\"closed\">\r\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        {{#each children}}\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n   \r\n        <div class=\"category-menu col-md-{{lang 'mega-menu.block-mega-1.col-menu-sub-category' }} {{lang 'mega-menu.block-mega-1.on-off-category' }}\" >\r\n            <section>\r\n                <div class=\"\" data-ap-category-url=\"{{url}}\" >\r\n                </div>\r\n            </section>\r\n        </div>\r\n    \r\n        <div class=\"html-menu col-md-{{lang 'mega-menu.block-mega-1.col-menu-sub-html'}} {{lang 'mega-menu.block-mega-1.on-off-html' }}\">\r\n            <div class=\"title-html-menu\">{{lang 'mega-menu.block-mega-1.title-html-menu'}}</div>\r\n            <div class=\"content-html-menu\">{{lang 'mega-menu.block-mega-1.content-html-menu'}}</div>\r\n            <div class=\"image-html-menu\">\r\n                <img src=\"{{lang 'mega-menu.block-mega-1.image-html-menu'}}\" alt=\"{{lang 'mega-menu.block-mega-1.alt-image' }}\">\r\n            </div>\r\n        </div> \r\n\r\n        <div class=\"video-menu col-md-{{lang 'mega-menu.block-mega-1.col-menu-sub-video'}} {{lang 'mega-menu.block-mega-1.on-off-video' }}\">\r\n            <div class=\"title-video-menu\">{{lang 'mega-menu.block-mega-1.title-video-menu'}}</div>\r\n            <div class=\"content-video-menu\">\r\n            <iframe width=\"100%\" height=\"250px\" src=\"{{lang 'mega-menu.block-mega-1.content-video-menu'}}\" frameborder=\"0\" allowfullscreen></iframe>\r\n            </div>\r\n        </div>\r\n\r\n    \r\n    {{else}}\r\n    <ul class=\"navPage-subMenu-list \">\r\n        <li class=\"navPage-subMenu-item\">\r\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a\r\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\r\n                        href=\"{{url}}\"\r\n                        data-collapsible=\"navPages-{{id}}\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\"\r\n                        data-collapsible-disabled-state=\"open\"\r\n                        data-collapsible-enabled-state=\"closed\">\r\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        {{#each children}}\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n    {{/if}}\r\n{{/if}}    \r\n"
}
